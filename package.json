    "d3-scale-chromatic": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "dirty-reprojectors": "0.0.2",
    "eslint": "^7.22.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-svelte3": "^3.1.2",
    "file-loader": "^6.2.0",
    "geo-albers-usa-territories": "^0.1.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "js-cookie": "^2.2.1",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lodash-es": "^4.17.21",
    "mapbox-gl": "^1.13.0",
    "marked": "^2.0.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-fetch": "^2.6.1",
    "nouislider": "^14.6.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.7",
    "release-it": "^14.4.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "seedrandom": "^3.0.5",
    "style-loader": "^2.0.0",
    "svelte": "^3.35.0",
    "svelte-icons": "^2.1.0",
    "svelte-jester": "^1.3.0",
    "svelte-loader": "^3.0.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "url-loader": "^4.1.1",
    "vega": "^5.20.0",
    "vega-embed": "^6.16.0",
    "vega-lite": "^5.0.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "uikit": "^3.6.18"
  },
  "lint-staged": {
    "*.{js,svelte}": "eslint --cache --fix",
    "*.{js,css,md,svelte}": "prettier --write"
  }
}
