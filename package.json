    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "bestzip": "^2.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "compute-logspace": "^1.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "cypress": "^6.6.0",
    "cypress-image-snapshot": "^4.0.1",
