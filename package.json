    "d3-format": "^2.0.0",
    "d3-random": "^2.2.2",
    "d3-scale": "^3.2.2",
    "d3-scale-chromatic": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "lodash-es": "^4.17.15",
