{
  "name": "www-covidcast",
  "version": "2.8.0",
  "private": true,
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/cmu-delphi/www-covidcast",
  "bugs": {
    "url": "https://github.com/cmu-delphi/www-covidcast/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmu-delphi/www-covidcast.git"
  },
  "main": "public/bundle.js",
  "files": [
    "public/**.js",
    "public/**.map",
    "public/**.txt",
    "public/**.css",
    "!public/wrapper.*"
  ],
  "scripts": {
    "clean": "rimraf \"*.zip\" \"*.tgz\" public",
    "compile": "tsc -p tsconfig.json --noEmit",
    "gen": "node ./scripts/generateDescriptions.js",
    "gen:check": "echo 'temporarily disabled'",
    "gen:local": "npm run gen -- --localOnly",
    "gen:fallback": "npm run gen -- --fallback",
    "start": "npm run gen:local && node --max-old-space-size=6192 node_modules/webpack-cli/bin/cli.js serve --mode development --hot --devtool eval-cheap-module-source-map",
    "prebuild": "npm run clean",
    "build": "npm run gen:local && cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development -w",
    "prettier": "prettier --write \"**/*\"",
    "lint": "npm run gen:check && eslint src --ext .js,.svelte && prettier --check \"**/*\"",
    "fix": "eslint src --ext .js,.svelte --fix && prettier --write \"**/*\"",
    "test": "jest --passWithNoTests",
    "serve": "http-server",
    "prepare": "husky install",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@octokit/request": "^5.6.0",
    "@popperjs/core": "^2.9.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/d3-color": "^2.0.1",
    "@types/d3-dsv": "^2.0.1",
    "@types/d3-format": "^2.0.0",
    "@types/d3-geo": "^2.0.0",
    "@types/d3-scale": "^3.3.0",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-time": "^2.1.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.4",
    "@types/lz-string": "^1.3.34",
    "@types/marked": "^2.0.3",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "d3-color": "^2.0.0",
    "d3-dsv": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.3.0",
    "d3-scale-chromatic": "^2.0.0",
    "d3-time": "^2.1.1",
    "d3-time-format": "^3.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "file-loader": "^6.2.0",
    "geo-albers-usa-territories": "^0.1.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "js-cookie": "^2.2.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "lodash-es": "^4.17.21",
    "marked": "^2.1.3",
    "mini-css-extract-plugin": "^2.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.7",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.0",
    "svelte-icons": "^2.1.0",
    "svelte-jester": "^1.7.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "vega": "^5.20.2",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "uikit": "^3.7.0"
  },
  "lint-staged": {
    "*.{ts,js,svelte}": "eslint --cache --fix",
    "*.{ts,js,css,md,svelte}": "prettier --write"
  }
}
