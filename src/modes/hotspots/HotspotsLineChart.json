{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": { "name": "values" },
  "width": "container",
  "height": "container",
  "autosize": {
    "resize": true
  },
  "encoding": {
    "color": {
      "value": "grey"
    },
    "x": {
      "field": "date_value",
      "type": "temporal",
      "axis": {
        "title": null,
        "format": "%b %e",
        "formatType": "time",
        "tickCount": "month"
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": null
    }
  },
  "mark": {
    "type": "line",
    "interpolate": "monotone"
  },
  "config": {
    "padding": 0,
    "view": {
      "stroke": null
    }
  }
}
